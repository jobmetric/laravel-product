{
    "name": "jobmetric/laravel-product",
    "description": "This is a product management package for Laravel that you can use in your projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "product",
        "management"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-product",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.19.3",
        "jobmetric/laravel-translation": "^2.22",
        "jobmetric/laravel-metadata": "^2.9",
        "jobmetric/laravel-comment": "^1.1",
        "jobmetric/laravel-like": "^1.4",
        "jobmetric/laravel-star": "^1.1.3",
        "jobmetric/laravel-layout": "^1.1",
        "jobmetric/laravel-url": "^1.8",
        "jobmetric/laravel-attribute": "^1.0",
        "jobmetric/laravel-taxonomy": "^3.1.2",
        "jobmetric/laravel-brand": "^1.2",
        "jobmetric/laravel-unit": "^1.3",
        "jobmetric/laravel-extension": "^1.3",
        "jobmetric/laravel-barcode": "^1.4.1",
        "jobmetric/laravel-language": "^1.13",
        "jobmetric/domi": "^1.13.1",
        "jobmetric/media": "^1.14",
        "jobmetric/panelio": "^1.16.1",
        "spatie/laravel-query-builder": "^5.7"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Product\\": "src/",
            "JobMetric\\Product\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Product\\ProductServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-product/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-product"
    }
}
